{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "./dist",
        "rootDir": "./src",
        "baseUrl": "./src",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "resolveJsonModule": true
    },
    "lib": [
        "ES6",
        "ES2017"
    ],
    "exclude": [
        "node_modules",
        "migrations",
        "knexfile.ts"
    ]
}